<section>
    <div class="container mt-5">
        <table class="table table-dark table-striped">
  <thead>
    <tr>
      
      <th scope="col">Name</th>
      <th scope="col">Dept</th>
      <th scope="col">Reg.no</th>
      <th scope="col">year</th>
    </tr>
  </thead>
  <tbody>
    {{#each users}}
    <tr>
      
      <td>{{this.name}}</td>
      <td>{{this.dept}}</td>
      <td>{{this.reg}}</td>
      <td>{{this.year}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>
    </div>
</section>